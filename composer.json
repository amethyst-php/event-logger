{
    "name": "railken/amethyst-event-logger",
    "keywords": ["laravel", "amethyst", "amethyst-event-logger", "event-logger"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "orchestra/testbench": "3.6.*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\EventLoggerServiceProvider"
            ]
        }
    }
}
