{
    "name": "amethyst/event-logger",
    "keywords": ["laravel", "amethyst", "amethyst-event-logger", "event-logger"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*"
    },
    "require-dev": {
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\EventLoggerServiceProvider"
            ]
        }
    }
}
